# Server Configuration
NODE_ENV=production
PORT=5000

# Database
MONGODB_URI=your_mongodb_connection_string_here
# OR for PostgreSQL (if you're using it)
# DATABASE_URL=your_postgresql_connection_string_here

# JWT Secret
JWT_SECRET=your_jwt_secret_key_here
JWT_EXPIRE=30d

# Frontend URL (for CORS)
# For local development, you don't need this as localhost is already allowed
# For production on Render, set this to your deployed frontend URL
# Example: https://your-frontend-app.onrender.com
FRONTEND_URL=https://your-frontend-app.onrender.com
